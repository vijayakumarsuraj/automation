-# Shows all test results.

- run_result = @content[:run_result]
- run_config = run_result.run_config
- application_name = run_config.application.application_name

.header
  = scoped_partial('header', application_name)
.content
  .iframe
    %iframe{src: link('allure/run', run_result.run_name, 'index.html'), width: '100%', height: '100%', seamless: ''}
